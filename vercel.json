{
  "buildCommand": "npm install && cd api && npm install && cd ../frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    }
  ]
}
